    "start": "NODE_ENV=production && node lib/server/start.js",
    "dev": "npm run build && node lib/server/start.js",
    "build": "rimraf lib && babel src --out-dir lib --copy-files",
    "pack:dev": "webpack --config config/webpack.dev-client.js --verbose",
    "pack:dev:server": "webpack --config config/webpack.dev-server.js",
    "pack:prod": "webpack --config config/webpack.prod-full.config.js",
    "pack:server": "webpack --config config/webpack.prod-client.js && webpack --config config/webpack.prod-server.js",
    "build-semantic": "cd semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && yarn run build-semantic && gulp watch",
    "start-semantic": "yarn run build-semantic && react-scripts start",
    "build-semantic:start": "yarn run build-semantic && react-scripts build",
